{
    "manifest_version": 2,
    "name":    "Webninja",
    "description": "Webninja",
    "version": "1.0",

	"browser_action": {
        "default_title": "Webninja",
        "default_icon" : "Ninja-icon.png"
    }, 
    
	
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	  },
    
	"web_accessible_resources": ["jquery-3.0.0.min.js", "jquery-2.1.1.min.js", "script.js"],
    "permissions": ["activeTab", "storage", "debugger", "tabs", "webNavigation", "http://*/", "https://*/"]
}