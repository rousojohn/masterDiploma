{
    "manifest_version": 2,
    "name":    "Test Extension",
    "version": "0.0",

    "background": {
        "persistent": false,
        "scripts": ["jquery-2.1.1.min.js", "background.js"]
    },
    "content_scripts": [{
        "matches": ["*://*.stackoverflow.com/*"],
        "js":      ["content.js"]
    }],
    "browser_action": {
        "default_title": "Test Extension"
    },

    "permissions": ["activeTab"]
}